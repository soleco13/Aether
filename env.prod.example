# Production Environment Variables for Docker Compose
# Copy this file to .env.prod and fill in your secure values

# Database
POSTGRES_PASSWORD=your-strong-postgres-password-here

# Django
DJANGO_SECRET_KEY=your-super-secret-django-key-here-at-least-50-characters-long

# MinIO Object Storage
MINIO_ROOT_USER=aether-admin
MINIO_ROOT_PASSWORD=your-strong-minio-password-here

# Keycloak Admin
KEYCLOAK_ADMIN_USER=admin
KEYCLOAK_ADMIN_PASSWORD=your-strong-keycloak-admin-password

# OIDC Client Secret
OIDC_CLIENT_SECRET=your-oidc-client-secret-from-keycloak

# Y-Provider API Key
Y_PROVIDER_API_KEY=your-y-provider-api-key 